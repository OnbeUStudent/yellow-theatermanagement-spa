{
  "consumer": {
    "name": "yellow-theatermanagement-spa"
  },
  "provider": {
    "name": "yellow-theatermanagement-bff"
  },
  "interactions": [
    {
      "description": "A GET request to retrieve the movies",
      "providerState": "There are SOME movies",
      "request": {
        "method": "GET",
        "path": "/api/movies"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "movieId": "FFFFFFFF-FFFF-FFFF-FFFF-FFFFFFFFFFFF",
            "title": "example-title"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}